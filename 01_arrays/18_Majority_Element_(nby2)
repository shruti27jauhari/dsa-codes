class Solution {
    public int majorityElement(int[] nums) {
      int i,count=0,candi=0,n=nums.length;
    for(i=0;i<n;i++){
        if(count==0){
            count++;
            candi=nums[i];
        }
        else if(nums[i]==candi){
            count++;
        }
        else
            count--;
        }
        
            return candi;
        
    }
        }
