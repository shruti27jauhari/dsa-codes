class Solution {
public:
    bool isAnagram(string s, string t) {
        if(s.length() != t.length()) return false;
        vector<int> freqcount(26,0);
        for(int i=0;i<s.length();i++){
            freqcount[s[i]-'a']++;
            freqcount[t[i]-'a']--;
        }
        for(int p: freqcount){
            if(p !=0) return false;
        }
          return true;
    }
};
