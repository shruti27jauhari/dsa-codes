class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int point = 0;
        for(int i=0; i<nums.size(); i++){
            if(nums[i] == 0){
                point++;
            }
            else{
                if(point != 0){
                    nums[i-point] = nums[i];
                    nums[i] = 0;
                }
                
            }
        }
    }
};
