class Solution {
public:
    int romanToInt(string s) {
        unordered_map<char,int> digi={
            {'I',1},{'V',5},{'X',10},{'L',50},{'C',100},{'D',500},{'M',1000}
        };
        int result=0;
        for(int i=0;i<s.length();i++){
            if(i+1<s.length() && digi[s[i]]<digi[s[i+1]]){
                result-=digi[s[i]];
            }
            else{
                 result+=digi[s[i]];
            }
        }
        return result;
    }
};
