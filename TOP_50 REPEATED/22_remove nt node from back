class Solution {
public:
    ListNode* removeNthFromEnd(ListNode* head, int n) {
        ListNode* fast=head;
        int i=0;
        while(i<n){
            fast=fast->next;
            i++;
        }
        if(fast==nullptr){
            return head->next;
        }
        ListNode* slow=head;
        while(fast->next != nullptr){
            slow=slow->next;
            fast=fast->next;
        }
        slow->next=slow->next->next;
        return head;
    }
};
