class Solution {
public:
    int search(vector<int>& nums, int target) {
        int n=nums.size(),ans,found=0;
        for(int i=0;i<n;i++){
            if(nums[i]==target){
                ans=i;
                found=1;
            }       
        }
        if(found==1) return ans;
        else return -1;
        
        
    }
};
