class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int>mapp;
        for(int i=0;i<nums.size();i++){
            if(mapp.count(target-nums[i])) return {i,mapp[target-nums[i]]};
            mapp[nums[i]]=i;
        }
        return{-1,-1};
    }
};
