#include <stack>
#include <cstdlib>
#include <string>
using namespace std;

int evalRPN(char** A, int n1) {
    stack<int> st;

    for (int i = 0; i < n1; i++) {
        string token = A[i];

      
        if (token == "+" || token == "-" || token == "*" || token == "/") {
            int b = st.top(); st.pop();
            int a = st.top(); st.pop();

            if (token == "+") st.push(a + b);
            else if (token == "-") st.push(a - b);
            else if (token == "*") st.push(a * b);
            else if (token == "/") st.push(a / b);
        }
        // operand
        else {
            st.push(atoi(A[i]));
        }
    }
    return st.top();
}
