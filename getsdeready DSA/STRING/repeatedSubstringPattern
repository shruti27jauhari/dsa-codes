class Solution {
public:
    bool repeatedSubstringPattern(string s) {
        int n=s.length();
        for(int i=n/2;i>0;i--){
            string st="";
            if(n%i==0){
                string subst=s.substr(0,i);
                int times=n/i;
                while(times--){
                    st=st+subst;
                }
                if(st==s)return true;
            }
        }
        return false;
    }
};
